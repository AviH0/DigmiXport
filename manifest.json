{
  "manifest_version": 2,
  "name": "HujiDigmiXport",
  "version": "1.2.3",
  "description": "Adds a browser action icon to the toolbar.",
  "icons": {
    "48": "icons/clock.png"
  },

  "permissions": [
    "cookies",
    "activeTab",
    "tabs",
    "webRequest",
    "<all_urls>"
  ],
  "browser_action": {
    "default_icon": "icons/clock.png",
    "default_title": "DigmiXport",
    "default_popup": "xport.html"
  }
}